{
    "buildCommand": "cd frontend && npm install && npm run build",
    "outputDirectory": "frontend/dist",
    "framework": null,
    "functions": {
        "api/[...path].js": {
            "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/[...path]"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}